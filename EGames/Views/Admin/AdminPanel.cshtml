@model EGames.Models.AdminPanelViewModel

<!DOCTYPE html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Dashboard - Index</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.ico">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <link rel="stylesheet" href="~/assets/admin/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/admin/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/assets/admin/css/owl.carousel.css">
    <link rel="stylesheet" href="~/assets/admin/css/owl.theme.css">
    <link rel="stylesheet" href="~/assets/admin/css/owl.transitions.css">
    <link rel="stylesheet" href="~/assets/admin/css/meanmenu/meanmenu.min.css">
    <link rel="stylesheet" href="~/assets/admin/css/animate.css">
    <link rel="stylesheet" href="~/assets/admin/css/normalize.css">
    <link rel="stylesheet" href="~/assets/admin/css/scrollbar/jquery.mCustomScrollbar.min.css">
    <link rel="stylesheet" href="~/assets/admin/css/jvectormap/jquery-jvectormap-2.0.3.css">
    <link rel="stylesheet" href="~/assets/admin/notika-custom-icon.css">
    <link rel="stylesheet" href="~/assets/admin/css/wave/waves.min.css">
    <link rel="stylesheet" href="~/assets/admin/css/main.css">
    <link rel="stylesheet" href="~/assets/admin/css/style.css">
    <link rel="stylesheet" href="~/assets/admin/css/responsive.css">
    <script src="~/assets/admin/js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>
    @if (!String.IsNullOrWhiteSpace(Model.DisplayMessage))
    {
        <script>
            $(document).ready(function () {
                $("#modalDisplayMessage").modal('show');
            });
        </script>
    }
    <div class="modal fade" id="modalDisplayMessage" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel">Alert</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    <h3>@Model.DisplayMessage</h3>
                </div>
                <!--Footer-->
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="header-top-area">
    </div>
    <!-- End Header Top Area -->
    <!-- Mobile Menu start -->
    <div class="mobile-menu-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="mobile-menu">
                        <nav id="dropdown">
                            <ul class="mobile-menu-nav">
                                @if (Model.IsAdmin)
                                {
                                    <li>
                                        <a data-toggle="collapse" data-target="#Charts" href="#">Admin</a>
                                        <ul class="collapse dropdown-header-top">
                                            <li><a asp-area="" asp-controller="Admin" asp-action="AdminPanel">Admin Panel</a></li>
                                        </ul>
                                    </li>
                                }
                                <li>
                                    <a data-toggle="collapse" data-target="#Charts" href="#">Home</a>
                                    <ul class="collapse dropdown-header-top">
                                        <li><a asp-area="" asp-controller="Admin" asp-action="Index">View Profile</a></li>
                                        <li><a asp-area="" asp-controller="Admin" asp-action="Logout">Logout</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a data-toggle="collapse" data-target="#demoevent" href="#">Games</a>
                                    <ul id="demoevent" class="collapse dropdown-header-top">
                                        <li><a asp-area="" asp-controller="Admin" asp-action="ColorBingo">Color Bingo</a></li>
                                        <li><a asp-area="" asp-controller="Admin" asp-action="BrainGame">Brain Game</a></li>
                                        <li><a asp-area="" asp-controller="Admin" asp-action="WordPuzzle">Word Puzzle</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalFundWallet" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel">Fund User Account</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    <form method="post" asp-action="FundWalletManually" asp-controller="Admin">
                        <h2 class="sr-only">Fund Wallet Manually</h2>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="emailaddress" placeholder="Email Address" asp-for="EmailAddress">
                            <span asp-validation-for="EmailAddress" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="amount" placeholder="Amount" asp-for="Amount">
                            <span asp-validation-for="Amount" class="text-danger"></span>
                        </div>
                        <div class="form-group"><button class="btn btn-primary btn-block" type="submit">Fund User Account</button></div>
                    </form>
                </div>
                <!--Footer-->
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalAddNotification" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel">Add Notification</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    <form method="post" asp-action="AddNotification" asp-controller="Admin">
                        <h2 class="sr-only">Add New Notification</h2>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="Message" placeholder="Message" asp-for="Message">
                            <span asp-validation-for="Message" class="text-danger"></span>
                        </div>
                        <div class="form-group"><button class="btn btn-primary btn-block" type="submit">Add Notification</button></div>
                    </form>
                </div>
                <!--Footer-->
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalAddBrainQuestion" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel">Add Brain Question</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    <form method="post" asp-action="AddBrainQuestion" asp-controller="Admin">
                        <h2 class="sr-only">Add New Notification</h2>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="Question" placeholder="Question" asp-for="Question">
                        </div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="Answer" placeholder="Answer (Multiple answers can be separated by using ';')" asp-for="Answer">
                        </div>
                        <div class="form-group">
                            @Html.DropDownListFor(m => m.BrainGameCategory, new SelectList(Enum.GetValues(typeof(EGamesData.Models.BrainGameCategory))), "Select Game Category")
                        </div>
                        <div class="form-group"><button class="btn btn-primary btn-block" type="submit">Add Brain Question</button></div>
                    </form>
                </div>
                <!--Footer-->
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalCheckWalletBalance" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel">Check Wallet Balance</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    <form method="post" asp-action="CheckUserAccountDetails" asp-controller="Admin">
                        <h2 class="sr-only">Add New Notification</h2>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="EmailAddress" placeholder="Email Address" asp-for="EmailAddress">
                            <span asp-validation-for="EmailAddress" class="text-danger"></span>
                        </div>
                        <div class="form-group"><button class="btn btn-primary btn-block" type="submit">Check Balance</button></div>
                    </form>
                </div>
                <!--Footer-->
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalMakeAgent" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel">Make User An Agent</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    <form method="post" asp-action="MakeAgent" asp-controller="Admin">
                        <h2 class="sr-only">Add New Notification</h2>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="EmailAddress" placeholder="Email Address" asp-for="EmailAddress">
                            <span asp-validation-for="EmailAddress" class="text-danger"></span>
                        </div>
                        <div class="form-group"><button class="btn btn-primary btn-block" type="submit">Make Agent</button></div>
                    </form>
                </div>
                <!--Footer-->
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalAddWordPuzzleQuestion" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel">Add Word Puzzle Question</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    <form method="post" asp-action="AddWordPuzzleQuestion" asp-controller="Admin">
                        <h2 class="sr-only">Add New Notification</h2>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="Question" placeholder="Question" asp-for="Question">
                        </div>
                        <div class="form-group">
                            <input class="form-control" type="text" name="Answer" placeholder="Answer (Multiple answers can be separated by using ';')" asp-for="Answer">
                        </div>
                        <div class="form-group"><button class="btn btn-primary btn-block" type="submit">Add Question</button></div>
                    </form>
                </div>
                <!--Footer-->
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Mobile Menu end -->
    <!-- Main Menu area start-->
    <div class="main-menu-area mg-tb-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                        @if (Model.IsAdmin)
                        {
                            <li class="active">
                                <a data-toggle="tab" href="#admin"><i class="notika-icon notika-mail"></i> Admin</a>
                            </li>
                        }
                        <li>
                            <a data-toggle="tab" href="#Home"><i class="notika-icon notika-house"></i> Home</a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#mailbox"><i class="notika-icon notika-mail"></i> Games</a>
                        </li>
                    </ul>
                    <div class="tab-content custom-menu-content">
                        <div id="admin" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                @if (Model.IsAdmin)
                                {
                                    <li><a asp-area="" asp-controller="Admin" asp-action="AdminPanel">Admin Panel</a></li>
                                }
                            </ul>
                        </div>
                        <div id="Home" class="tab-pane notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a asp-area="" asp-controller="Admin" asp-action="Index">View Profile</a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="Admin" asp-action="Logout">Logout</a>
                                </li>
                            </ul>
                        </div>
                        <div id="mailbox" class="tab-pane in active notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a asp-area="" asp-controller="Admin" asp-action="ColorBingo">Color Bingo</a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="Admin" asp-action="BrainGame">Brain Game</a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="Admin" asp-action="WordPuzzle">Word Puzzle</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Main Menu area End-->
    <!-- Start Status area -->
    <div class="notika-status-area">
        <div class="container">
        </div>
    </div>
    <!-- End Status area-->
    <!-- Start Sale Statistic area-->
    <div class="sale-statistic-area">
        <div class="container">
            <h3 style="color:red; font-size:medium">@Model.ErrorMessage</h3>
            <h3 style="color:green; font-size:medium">@Model.SuccessMessage</h3>
            <div class="row">
                <!-- Place Bingo Here-->
                <div class="col-lg-9 col-md-8 col-sm-7 col-xs-12">
                    <div class="sale-statistic-inner notika-shadow mg-tb-30">
                        <div class="col">
                            <div class="form-check">
                                <p>Total Users Registered: @Model.TotalUsersRegistered</p>
                            </div>
                            <br />
                            <div class="form-check">
                                <button id="btnCheckUserBalance" class="btn btn-info" type="button" onclick="GetCheckWalletBalancePartial()">Check User Balance</button>
                            </div>
                            <br />
                            <div class="form-check">
                                <button id="btnMakeAgent" class="btn btn-info" type="button" onclick="GetMakeAgentPartial()">Make User An Agent</button>
                            </div>
                            <br />
                            <div class="form-check">
                                <button id="btnFundWallet" class="btn btn-info" type="button" onclick="GetFundWalletPartial()">Fund Account</button>
                            </div>
                            <br />
                            <div class="form-check">
                                <button id="btnAddNotification" class="btn btn-info" type="button" onclick="GetAddNotificationPartial()">Add Notification</button>
                            </div>
                            <br />
                            <div class="form-check">
                                <button id="btnAddBrainQuestion" class="btn btn-info" type="button" onclick="GetAddBrainQuestionPartial()">Add Brain Question</button>
                            </div>
                            <br />
                            <div class="form-check">
                                <button id="btnAddWordPuzzleQuestion" class="btn btn-info" type="button" onclick="GetAddWordPuzzleQuestionPartial()">Add Word Puzzle Question</button>
                            </div>
                            <br />
                            <div class="form-check">
                                <label class="switch">
                                    <input type="checkbox" onchange="showPendingPayoutList(this)">
                                    <span class="slider round"></span>Show Pending Payout List
                                </label>
                            </div>
                            <div class="form-check">
                                <label class="switch">
                                    <input type="checkbox" onchange="showBrainQuestionList(this)">
                                    <span class="slider round"></span>Show Brain Question List
                                </label>
                            </div>
                            <div class="form-check">
                                <label class="switch">
                                    <input type="checkbox" onchange="showWordPuzzleQuestionList(this)">
                                    <span class="slider round"></span>Show Word Puzzle Question List
                                </label>
                            </div>
                            <div class="form-check">
                                <label class="switch">
                                    <input type="checkbox" onchange="showAgentListList(this)">
                                    <span class="slider round"></span>Show Agent List
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-4 col-sm-5 col-xs-12">
                    <div class="statistic-right-area notika-shadow mg-tb-30 sm-res-mg-t-0">
                        <div class="past-day-statis">
                            <h2>Admin Panel</h2>
                            <ul>
                                <li>-Your an admin, means your a boss</li>
                                <li>-Being a boss means you have full control of the system</li>
                                <li>-Lets make money Boss ;-) </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Sale Statistic area-->

    <div class="notika-email-post-area">
        <div class="container">
            <div class="row" id="rowPayouts" hidden="hidden">
                <div class="recent-post-wrapper notika-shadow sm-res-mg-t-30 tb-res-ds-n dk-res-ds">
                    <div class="recent-post-ctn">
                        <div class="recent-post-title">
                            <h2>Payout List</h2>
                            <p>Total Users Pending Payout : @Model.UsersPendingPayout.Count()</p>
                        </div>
                    </div>
                    <div class="recent-post-items">
                        <div class="recent-post-signle rct-pt-mg-wp">
                            <div class="body table-responsive">
                                <table class="table table-condensed">
                                    <thead>
                                        <tr>
                                            <th>Email Address</th>
                                            <th>Account Number</th>
                                            <th>Bank Name</th>
                                            <th>Amount</th>
                                            @if (Model.IsAdmin)
                                            {
                                            <th></th>
                                            }
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var user in Model.UsersPendingPayout)
                                        {
                                        <tr>
                                            <td>@user.EmailAddress</td>
                                            <td>@user.AccountNumber</td>
                                            <td>@user.BankName</td>
                                            <td>@user.PendingWithdrawalAmount</td>
                                            @if (Model.IsAdmin)
                                                {
                                            <td><input type="button" value="Paid" onclick="location.href='@Url.Action("UserPaid", "Admin", new { userId = user.Id })'" /></td>
                                                }
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row" id="rowBrainQuestions" hidden="hidden">
                <div class="recent-post-wrapper notika-shadow sm-res-mg-t-30 tb-res-ds-n dk-res-ds">
                    <div class="recent-post-ctn">
                        <div class="recent-post-title">
                            <h2>Brain Game Questions</h2>
                            <p>Total DC Movie Questions @Model.AvailableBrainGameQuestions.Where(x => x.BrainGameCategory == EGamesData.Models.BrainGameCategory.DCMovies).Count()</p>
                            <p>Total Marvel Movie Questions @Model.AvailableBrainGameQuestions.Where(x => x.BrainGameCategory == EGamesData.Models.BrainGameCategory.MarvelMovies).Count()</p>
                            <p>Total Football Questions @Model.AvailableBrainGameQuestions.Where(x => x.BrainGameCategory == EGamesData.Models.BrainGameCategory.Football).Count()</p>
                            <p>Total Music Questions @Model.AvailableBrainGameQuestions.Where(x => x.BrainGameCategory == EGamesData.Models.BrainGameCategory.Music).Count()</p>
                        </div>
                    </div>
                    <div class="recent-post-items">
                        <div class="recent-post-signle rct-pt-mg-wp">
                            <div class="body table-responsive">
                                <table class="table table-condensed">
                                    <thead>
                                        <tr>
                                            <th>Added By</th>
                                            <th>Question</th>
                                            <th>Answers</th>
                                            <th>Game Category</th>
                                            @if (Model.IsAdmin)
                                            {
                                            <th></th>
                                            }
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var brainGameQuestion in Model.AvailableBrainGameQuestions)
                                        {
                                        <tr>
                                            <td>@brainGameQuestion.AddedBy.EmailAddress</td>
                                            <td>@brainGameQuestion.Question</td>
                                            <td>@brainGameQuestion.Answers</td>
                                            <td>@brainGameQuestion.BrainGameCategory.ToString()</td>
                                            @if (Model.IsAdmin)
                                            {
                                                <td><input type="button" value="Remove" onclick="location.href='@Url.Action("RemoveBrainGameQuestion", "Admin", new { brainGameQuestionId = brainGameQuestion.Id })'" /></td>
                                            }
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row" id="rowAgentList" hidden="hidden">
                <div class="recent-post-wrapper notika-shadow sm-res-mg-t-30 tb-res-ds-n dk-res-ds">
                    <div class="recent-post-ctn">
                        <div class="recent-post-title">
                            <h2>Agent List</h2>
                        </div>
                    </div>
                    <div class="recent-post-items">
                        <div class="recent-post-signle rct-pt-mg-wp">
                            <div class="body table-responsive">
                                <table class="table table-condensed">
                                    <thead>
                                        <tr>
                                            <th>Email</th>
                                            <th>Account No</th>
                                            <th>Bank</th>
                                            <th>Agent Code</th>
                                            <th>Total Users Referred</th>
                                            @*@if (Model.IsAdmin)
                                        {
                                            <th></th>
                                        }*@
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var agent in Model.AgentList)
                                        {
                                        <tr>
                                            <td>@agent.Key.EmailAddress</td>
                                            <td>@agent.Key.AccountNumber</td>
                                            <td>@agent.Key.BankName</td>
                                            <td>@agent.Key.AgentCode</td>
                                            <td>@agent.Value.ToString()</td>
                                            @*@if (Model.IsAdmin)
                                        {
                                            <td><input type="button" value="Remove" onclick="location.href='@Url.Action("RemoveBrainGameQuestion", "Admin", new { brainGameQuestionId = brainGameQuestion.Id })'" /></td>
                                        }*@
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row" id="rowWordPuzzleQuestionList" hidden="hidden">
                <div class="recent-post-wrapper notika-shadow sm-res-mg-t-30 tb-res-ds-n dk-res-ds">
                    <div class="recent-post-ctn">
                        <div class="recent-post-title">
